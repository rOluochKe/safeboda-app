# Database Configuration
DB_HOST=your_database_host
DB_PORT=your_database_port
DB_USERNAME=your_database_username
DB_PASSWORD=your_database_password
DB_DATABASE=your_database_name

# JWT Secret Key (Replace with a secure secret key)
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRATION_TIME=3600  # Token expiration time in seconds (e.g., 1 hour)

# Server Port (Optional: Replace with your desired port number)
PORT=3000

# Logging Configuration (Optional)
LOG_LEVEL=debug  # Change to 'error', 'warn', 'info', or 'debug' as needed
